<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd">

	<module name="message" package="com.axelor.apps.message.db" />

	<entity name="Message" lang="java">
		<one-to-many name="office365MessageList" ref="com.axelor.apps.office.db.Office365Message" mappedBy="message"/>
		<boolean name="isOffice365Object"/>
		<datetime name="receivedDateT" title="Received date" readonly="true"/>
	</entity>

</domain-models>